{{#if parts.length}}
<div class="dice-tooltip">
	<div class="wrapper">
		<section class="tooltip-part">
			<div class="dice">
				<ol class="dice-rolls">
				{{#each parts}}
					{{#each this.rolls}}
					<li class="roll {{this.classes}}">{{{this.result}}}</li>
					{{/each}}
				{{/each}}
				</ol>
			</div>
		</section>
	</div>
</div>
{{/if}}
