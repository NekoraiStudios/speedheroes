<div class="dice-roll" data-action="expandRoll">
	{{#if flavor}}
	<div class="dice-flavor">{{flavor}}</div>
	{{/if}}
	<div class="dice-result">
		{{#if parts.length}}
			{{#each parts}}
				<section class="dice-part">
					<div class="dice">
						<header class="part-header flexrow">
							{{#if icon}}
							<span class="part-method" data-tooltip aria-label="{{ localize method }}">
								{{{ icon }}}
							</span>
							{{/if}}
							{{#if this.flavor}}<span class="part-flavor">{{this.flavor}}</span>{{/if}}
							<span class="part-total">{{this.total}}</span>
							test
						</header>
						<ol class="dice-rolls">
						{{#each this.rolls}}
						<li class="roll {{this.classes}}">{{{this.result}}}</li>
						{{/each}}
						</ol>
					</div>
				</section>
			{{/each}}
		{{/if}}

		{{{tooltip}}}
		<h4 class="dice-total">{{total}}</h4>
	</div>
</div>
